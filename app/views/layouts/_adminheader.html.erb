                        <li> <%= link_to new_hospitaladmin_registration_path, :data =>{ :no_turbolink => true } do %>
                            <i class="fa fa-medkit"></i><span class="txt">Add Hospital</span>
                         <% end %>   
                        </li>
                        
                     
                    </ul>
                    <div class="clearfix"></div>
                </div>
                <!-- End .sidebar-scrollarea -->
            </div>
            <!-- End .sidebar-inner -->
        </aside>
        <!-- End #sidebar -->
      
        <!-- End .sidebar-inner -->
        </aside>
