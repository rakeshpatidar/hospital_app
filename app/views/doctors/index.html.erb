<p id="notice"><%= notice %></p>

<h1>Listing Doctors</h1>

<table>
  <thead>
    <tr>
      <th>First-name</th>
      <th>Last-name</th>
      <th>Gender</th>
      <th>Email</th>
      <th>Address</th>
      <th>Language</th>
      <th>Physician-type</th>
      <th>Contact-no</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @doctors.each do |doctor| %>
      <tr>
        <td><%= doctor.first-name %></td>
        <td><%= doctor.last-name %></td>
        <td><%= doctor.gender %></td>
        <td><%= doctor.email %></td>
        <td><%= doctor.address %></td>
        <td><%= doctor.language %></td>
        <td><%= doctor.physician-type %></td>
        <td><%= doctor.contact-no %></td>
        <td><%= link_to 'Show', doctor %></td>
        <td><%= link_to 'Edit', edit_doctor_path(doctor) %></td>
        <td><%= link_to 'Destroy', doctor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Doctor', new_doctor_path %>
